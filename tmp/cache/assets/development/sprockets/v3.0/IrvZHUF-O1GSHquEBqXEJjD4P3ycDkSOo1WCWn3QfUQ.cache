<div class="row">
  <div class="table-responsive">
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
              <th>Sr</th>
              <th>Name</th>
              <th>Email</th>
              <th>Contact</th>
              <th>Actions</th>
            </tr>
        </thead>
        <tbody>
          <tr ng-repeat="customer in customers">
            <td>SR</td>
            <td>{{customer.name}}</td>
            <td>{{customer.email}}</td>
            <td>{{customer.contact}}</td>
            <td>Actions</td>
          </tr>
        </tbody>
    </table>
  </div>
</div>
