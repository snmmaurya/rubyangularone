<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
           Customers Management System<br>
           <form ng-controller="customerController" name="customerForm" novalidate>
            Name<input type="text" ng-model="customerForm.name"><br>
            Emai<input type="text" ng-model="customerForm.email"><br>
            Contact<input type="text" ng-model="customerForm.contact"><br>
            <button type="submit" class="btn btn-primary" ng-click="createContact(contactData)" >Send Message</button>
            <intput type="submit" value="Add Customer" name="Add Customer" ng-click="add_customer(customerForm)">
           </form>
           <br>Search Filter By Name:
           <input type="text" ng-model="serachFilter">
        </h1>                
    </div>
</div>
<!-- /.row -->

<!-- Content -->
<div class="row">
  <div class="table-responsive">
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th>Sr</th>
                <th>Title</th>
                <th>Dev Type</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        <% if @developments.present? %>
        <% counter = 0 %>
          <% @developments.each do |development| %>
              <tr>
                  <td><%= counter = counter + 1 %></td>
                  <td><%= development.title %></td>
                  <td><%= development.dev_type %></td>
                  <td>
                    <%= link_to :view, administrator_development_path(development), class: "btn btn-xs btn-default" %>
                    <%= link_to :edit, edit_administrator_development_path(development), class: "btn btn-xs btn-info" %>
                    <%= link_to :delete, administrator_development_path(development), method: :delete, data: {confirm: "Are you sure!"}, class: "btn btn-xs btn-danger" %>
                  </td>                           
              </tr>
          <% end %>
        <% else %>
          <tr><td colspan="5" class="text-center">Data not found!</td></tr>  
        <% end %>              
        </tbody>
    </table>
  </div>
</div>
