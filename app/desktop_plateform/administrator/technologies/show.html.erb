<!-- Page Heading/Breadcrumbs -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
           Technology
           <small>Show Technology</small>
        </h1>                
    </div>
</div>
<!-- /.row -->

<!-- Technology Content -->
<div class="row">
  <div class="control-group form-group">
		<div class="controls">
  		<label>Title</label>
  		<%= @technology.title %>
		</div>
    <div class="controls">
      <label>Tech Type</label>
      <%= @technology.tech_type %>
    </div>
		<div class="controls">
			<%= link_to :Back, administrator_technologies_path, class: "btn btn-default" %>			
			<%= link_to :Edit, edit_administrator_technology_path(@technology), class: "btn btn-info" %>
      <%= link_to :Destroy, administrator_technology_path(@technology), method: :delete, data: {confirm: "Are you sure!"}, class: "btn btn-danger" %>
		</div>
   </div>
</div>