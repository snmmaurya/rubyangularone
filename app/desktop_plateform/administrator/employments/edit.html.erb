<!-- Page Heading/Breadcrumbs -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
           Employment
           <small>Update Employment</small>
        </h1>                
    </div>
</div>
<!-- /.row -->

<!-- Employment Content -->
<div class="row">
  <%= form_for([:administrator, @employment]) do |form| %>
    <%= render partial: "shared/error_messages", locals: {error: @employment}  %>    
    <div class="control-group form-group">
      <%= render partial: "form", locals: {form: form, buttons: link_to(:Show, administrator_employment_path(@employment), class: "btn btn-default")} %>      
    </div>
  <% end %>
</div>