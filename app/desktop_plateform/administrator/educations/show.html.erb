<!-- Page Heading/Breadcrumbs -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
           Education
           <small>Show Education</small>
        </h1>                
    </div>
</div>
<!-- /.row -->

<!-- Education Content -->
<div class="row">
  <div class="control-group form-group">
		<div class="controls">
  		<label>Title</label>
  		<%= @education.title %>
		</div>
    <div class="controls">
      <label>College</label>
      <%= @education.school %>
    </div>
    <div class="controls">
      <label>Status</label>
      <%= @education.status %>
    </div>
    <div class="controls">
      <label>Description</label>
      <%= @education.description %>
    </div>
    <div class="controls">
      <label>Started At</label>
      <%= @education.started_at %>
    </div>
    <div class="controls">
      <label>Completed At</label>
      <%= @education.completed_at %>
    </div>
		<div class="controls">
			<%= link_to :Back, administrator_educations_path, class: "btn btn-default" %>			
			<%= link_to :Edit, edit_administrator_education_path(@education), class: "btn btn-info" %>
      <%= link_to :Destroy, administrator_education_path(@education), method: :delete, data: {confirm: "Are you sure!"}, class: "btn btn-danger" %>
		</div>
   </div>
</div>