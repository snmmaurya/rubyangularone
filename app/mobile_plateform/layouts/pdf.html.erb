<!DOCTYPE html>
<html>
<head>
  <title>SnmMaurya</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= render "shared/menu.html.erb" %>
	<div id="flash-messages-container">
		<%= render "shared/flash_messages" %>
	</div>
	<div class="container">	
		<%= yield %>
		<hr>
		<!-- Footer -->
		<footer>
			<div class="row">
				<div class="col-lg-12">
					<p>Copyright &copy; SnmMaurya.com 2015-2020</p>
				</div>
			</div>
		</footer>
	</div>
	<!-- /.container -->
	<%= yield :script %>
</body>
</html>
